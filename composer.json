{
    "name": "lewestopher/cakephp-push",
    "description": "Push notification plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "mockery/mockery": "0.9.*",
        "pusher/pusher-php-server": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeMonga\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakePush\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
